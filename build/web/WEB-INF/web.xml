<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InicioSesion</servlet-name>
        <servlet-class>servlet.InicioSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarUsuarios</servlet-name>
        <servlet-class>servlet.RegistrarUsuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarSesion</servlet-name>
        <servlet-class>servlet.CerrarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultaEstudiantesCola</servlet-name>
        <servlet-class>servlet.ConsultaEstudiantesCola</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultaEstudiantes</servlet-name>
        <servlet-class>servlet.ConsultaEstudiantes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarAlumno</servlet-name>
        <servlet-class>servlet.RegistrarAlumno</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddColaEstudiantes</servlet-name>
        <servlet-class>servlet.AddColaEstudiantes</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InicioSesion</servlet-name>
        <url-pattern>/iniciar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarUsuarios</servlet-name>
        <url-pattern>/nuevousuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarSesion</servlet-name>
        <url-pattern>/cerrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultaEstudiantesCola</servlet-name>
        <url-pattern>/ConsultaEstudiantesCola</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultaEstudiantes</servlet-name>
        <url-pattern>/ConsultaEstudiantes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarAlumno</servlet-name>
        <url-pattern>/RegistrarAlumno</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddColaEstudiantes</servlet-name>
        <url-pattern>/AddColaEstudiantes</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
